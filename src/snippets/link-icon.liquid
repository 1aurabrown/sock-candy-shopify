{% assign classes = 'inline-block ml-1 -mt-1 w-4 h-4 md:ml-2 md:w-6 md:h-6' %}
{% if link.title == 'facebook' or link.title == 'Facebook' %}
  {% include 'icon-facebook' class: classes %}
{% elsif link.title == 'instagram' or link.title == 'Instagram' %}
  {% include 'icon-instagram', class: classes %}
{% endif %}

{% assign classes = blank %}